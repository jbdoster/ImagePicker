 {
    "name": "ImagePicker",
    "version": "1.0",
    "description": "",
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
      "activeTab",
      "tabs",
      "storage"
    ],
    "content_scripts": [{
      "matches":    ["http://*/*", "https://*/*"],
      "js":         ["content.js"],
      "run_at": "document_end",
      "all_frames": true
   }],
   "web_accessible_resources": [
      "content.js"
    ]
  }
